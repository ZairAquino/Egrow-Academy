{
  "name": "egrow-academy-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:turbo": "next dev --turbopack",
    "dev:fast": "next dev --turbopack --port 3000",
    "dev:minimal": "NODE_ENV=development next dev --turbopack --port 3000",
    "dev:no-cache": "next dev --turbopack --port 3000 --clear",
    "dev:optimized": "cross-env NODE_ENV=development next dev --turbopack --port 3000",
    "build": "npx prisma generate && npx next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "init-stripe": "tsx scripts/init-stripe-products.ts",
    "test-stripe": "tsx scripts/test-stripe-config.ts",
    "check-prod-env": "tsx scripts/check-production-env.ts",
    "create-test-coupon": "tsx scripts/create-test-coupon.ts",
    "send-coupon-email": "tsx scripts/send-coupon-email.ts",
    "restore-email-verification": "tsx scripts/restore-email-verification.ts",
    "fix-build": "tsx scripts/fix-build-issues.ts",
    "prepare-deploy": "tsx scripts/prepare-for-deploy.ts",
    "generate-sitemap": "next-sitemap",
    "generate-seo-files": "node scripts/generate-seo-files.js",
    "analyze": "ANALYZE=true npm run build",
    "seo-audit": "npm run generate-sitemap && npm run analyze",
    "lighthouse": "lighthouse https://egrow-academy.com --output=json --output-path=./lighthouse-report.json",
    "seo-check": "npm run generate-sitemap && npm run lighthouse",
    "seo-setup": "npm run generate-seo-files && npm run generate-sitemap",
    "seo-analysis": "node scripts/seo-analysis.js",
    "test-ga4": "tsx scripts/test-ga4.ts",
    "seo-full": "npm run generate-seo-files && npm run seo-analysis && npm run lighthouse",
    "seo-monitor": "npm run seo-analysis && npm run lighthouse",
    "measure-cwv": "node scripts/measure-cwv.js",
    "performance-analysis": "node scripts/performance-analysis.js",
    "optimize-performance": "node scripts/optimize-performance.js",
    "seed-courses": "tsx scripts/seed-courses.ts",
    "verify-platform": "tsx scripts/verify-platform.ts",
    "backup-database": "tsx scripts/backup-database.ts",
    "backup-database-prisma": "tsx scripts/backup-database-prisma.ts",
    "migrate-backups": "tsx scripts/migrate-backups-to-local.ts",
    "verify-backup-locations": "tsx scripts/verify-backup-locations.ts",
    "security-audit": "tsx scripts/security-audit.ts",
    "monitor-security": "tsx scripts/monitor-security.ts",
    "debug:server": "node mcp-debug-server.js",
    "debug:inspect": "npx @modelcontextprotocol/inspector node mcp-debug-server.js",
    "health": "curl http://localhost:3000/api/health || echo 'Server not running'",
    "kill-port": "npx kill-port 3000",
    "dev:clean": "npm run kill-port && npm run dev:fast",
    "dev:debug": "concurrently \"npm run dev:fast\" \"npm run debug:inspect\"",
    "fix-deps": "rm -rf node_modules package-lock.json && npm install",
    "typecheck": "tsc --noEmit",
    "dev:safe": "npm run dev:clean",
    "dev:with-checks": "npm run typecheck && npm run lint && npm run dev:clean",
    "quick-fix": "tsx scripts/fix-typescript-errors.ts",
    "setup-recommendations": "tsx scripts/setup-recommendations-db.ts",
    "setup-achievements": "tsx scripts/setup-achievements-db.ts",
    "update-courses-categories": "tsx scripts/update-courses-categories.ts",
    "generate-social-urls": "tsx scripts/generate-social-urls.ts",
    "cleanup-webinars:dry": "tsx scripts/cleanup-webinars.ts --dry-run --keep-slug=videos-profesionales-ia",
    "cleanup-webinars": "tsx scripts/cleanup-webinars.ts --keep-slug=videos-profesionales-ia",
    "check-webinars": "tsx scripts/check-webinars.ts",
    "test-utm-tracking": "tsx scripts/test-utm-tracking.ts",
    "get-social-analytics": "tsx scripts/get-social-analytics.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.16.2",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@prisma/client": "^6.13.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "@tabler/icons-react": "3.34.1",
    "@uploadthing/react": "^7.3.2",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "email-validator": "^2.0.4",
    "framer-motion": "^12.23.12",
    "geist": "^1.4.2",
    "helmet": "^8.1.0",
    "hugeicons-react": "^0.3.0",
    "jsonwebtoken": "^9.0.2",
    "kill-port": "^2.0.1",
    "lucide-react": "^0.525.0",
    "next": "^15.4.1",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "resend": "^4.7.0",
    "simple-icons": "^15.10.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/debug": "^4.1.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "lighthouse": "^12.8.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
