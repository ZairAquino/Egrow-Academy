{
  "images": {
    "formats": [
      "webp",
      "avif"
    ],
    "lazyLoading": true,
    "responsive": true
  },
  "css": {
    "critical": true,
    "minify": true,
    "purge": true
  },
  "javascript": {
    "codeSplitting": true,
    "treeShaking": true,
    "defer": true
  },
  "server": {
    "compression": true,
    "caching": true,
    "cdn": false
  }
}